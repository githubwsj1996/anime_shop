<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sj.repository.GoodsCategoryRepository">
    <select id="findAllCate" resultType="AnimeCategory">
        select * from anime_category
    </select>

    <select id="findAllById" resultType="AnimeCategory">
        select * from anime_category where category_id=#{param1}
    </select>

</mapper>